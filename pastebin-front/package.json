{
    "name": "pastebin-front",
    "version": "0.4.0",
    "private": true,
    "scripts": {
        "dev": "vite --open",
        "build": "run-p fmt-check lint type-check build-only",
        "preview": "vite preview --open",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "fmt": "prettier --write src/ *.ts *.cjs *.json *.html",
        "fmt-check": "prettier --check src/ *.ts *.cjs *.json *.html"
    },
    "dependencies": {
        "pinia": "^2.0.32",
        "vue": "^3.2.47",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.2.0",
        "@types/node": "^18.14.2",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitejs/plugin-vue-jsx": "^3.0.0",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.34.0",
        "eslint-plugin-vue": "^9.9.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.4",
        "typescript": "~4.8.4",
        "vite": "^4.1.4",
        "vue-tsc": "^1.2.0"
    }
}
